{
  "name": "crello",
  "version": "0.1.0",
  "author": "Ninad Subba <ninuch7@gmail.com>",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build",
    "build:prod": "npm run config -- --environment=prod && ng build --prod",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test",
    "lint": "eslint src/**/*.ts",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm run test && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.6",
    "@angular/cdk": "^9.2.4",
